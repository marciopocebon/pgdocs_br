msgid "-p"
msgstr ""

#: xml/vacuumlo.xml:130(replaceable)
msgid "port"
msgstr ""

#: xml/vacuumlo.xml:133(para)
msgid "Database server's port."
msgstr ""

#: xml/vacuumlo.xml:138(option)
