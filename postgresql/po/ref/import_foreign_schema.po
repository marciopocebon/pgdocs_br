#: xml/ref/import_foreign_schema.xml:17(synopsis)
#, no-wrap
msgid "IMPORT FOREIGN SCHEMA \n    <placeholder-1/>[ {\n    LIMIT TO | EXCEPT } ( \n    <placeholder-2/>[, ...]\n    ) ] FROM SERVER \n    <placeholder-3/>INTO \n    <placeholder-4/>[\n    OPTIONS ( \n    <placeholder-5/>' \n    <placeholder-6/>' [, ... ] )\n    ]"
msgstr ""

#: xml/ref/import_foreign_schema.xml:30(title)
msgid "Description"
msgstr ""

#: xml/ref/import_foreign_schema.xml:31(para)
msgid "<command>IMPORT FOREIGN SCHEMA</command>creates foreign tables that represent tables existing on a foreign server. The new foreign tables will be owned by the user issuing the command and are created with the correct column definitions and options to match the remote tables."
msgstr ""

#: xml/ref/import_foreign_schema.xml:37(para)
msgid "By default, all tables and views existing in a particular schema on the foreign server are imported. Optionally, the list of tables can be limited to a specified subset, or specific tables can be excluded. The new foreign tables are all created in the target schema, which must already exist."
msgstr ""

#: xml/ref/import_foreign_schema.xml:42(para)
msgid "To use <command>IMPORT FOREIGN SCHEMA</command>, the user must have <literal>USAGE</literal>privilege on the foreign server, as well as <literal>CREATE</literal>privilege on the target schema."
msgstr ""

#: xml/ref/import_foreign_schema.xml:49(title)
msgid "Parameters"
msgstr ""

#: xml/ref/import_foreign_schema.xml:57(para)
msgid "The remote schema to import from. The specific meaning of a remote schema depends on the foreign data wrapper in use."
msgstr ""

#: xml/ref/import_foreign_schema.xml:64(literal)
msgid "LIMIT TO ( <placeholder-1/>[, ...] )"
msgstr ""

#: xml/ref/import_foreign_schema.xml:69(para)
msgid "Import only foreign tables matching one of the given table names. Other tables existing in the foreign schema will be ignored."
msgstr ""

#: xml/ref/import_foreign_schema.xml:76(literal)
msgid "EXCEPT ( <placeholder-1/>[, ...] )"
msgstr ""

#: xml/ref/import_foreign_schema.xml:81(para)
msgid "Exclude specified foreign tables from the import. All tables existing in the foreign schema will be imported except the ones listed here."
msgstr ""

#: xml/ref/import_foreign_schema.xml:91(para)
msgid "The foreign server to import from."
msgstr ""

#: xml/ref/import_foreign_schema.xml:99(para)
msgid "The schema in which the imported foreign tables will be created."
msgstr ""

#: xml/ref/import_foreign_schema.xml:105(literal)
msgid "OPTIONS ( <placeholder-1/>' <placeholder-2/>' [, ...] )"
msgstr ""

#: xml/ref/import_foreign_schema.xml:111(para)
msgid "Options to be used during the import. The allowed option names and values are specific to each foreign data wrapper."
msgstr ""

#: xml/ref/import_foreign_schema.xml:119(title)
msgid "Examples"
msgstr ""

#: xml/ref/import_foreign_schema.xml:125(programlisting)
#, no-wrap
msgid "IMPORT FOREIGN SCHEMA foreign_films FROM SERVER\n    film_server INTO films;"
msgstr ""

#: xml/ref/import_foreign_schema.xml:120(para)
msgid "Import table definitions from a remote schema <structname>foreign_films</structname>on server <structname>film_server</structname>, creating the foreign tables in local schema <structname>films</structname>: <placeholder-1/>"
msgstr ""

#: xml/ref/import_foreign_schema.xml:130(programlisting)
#, no-wrap
msgid "IMPORT FOREIGN SCHEMA foreign_films LIMIT TO\n    (actors, directors) FROM SERVER film_server INTO\n    films;"
msgstr ""

#: xml/ref/import_foreign_schema.xml:127(para)
msgid "As above, but import only the two tables <structname>actors</structname>and <literal>directors</literal>(if they exist): <placeholder-1/>"
msgstr ""

#: xml/ref/import_foreign_schema.xml:135(title)
msgid "Compatibility"
msgstr ""

#: xml/ref/import_foreign_schema.xml:136(para)
msgid "The <command>IMPORT FOREIGN SCHEMA</command>command conforms to the <acronym>SQL</acronym>standard, except that the <literal>OPTIONS</literal>clause is a <productname>PostgreSQL</productname>extension."
msgstr ""

