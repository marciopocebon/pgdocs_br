msgid "Notes"
msgstr ""

#: xml/ref/abort.xml:46(para)
msgid "Use <xref linkend=\"SQL-COMMIT\">to successfully terminate a transaction.</xref>"
msgstr ""

#: xml/ref/abort.xml:49(para)
msgid "Issuing <command>ABORT</command>outside of a transaction block emits a warning and otherwise has no effect."
msgstr ""

