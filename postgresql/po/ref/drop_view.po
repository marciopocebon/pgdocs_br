#: xml/ref/drop_view.xml:4(primary) xml/ref/drop_view.xml:7(refentrytitle) xml/ref/drop_view.xml:12(refname)
msgid "DROP VIEW"
msgstr ""

#: xml/ref/drop_view.xml:8(manvolnum)
msgid "7"
msgstr ""

#: xml/ref/drop_view.xml:9(refmiscinfo)
msgid "SQL - Language Statements"
msgstr ""

#: xml/ref/drop_view.xml:13(refpurpose)
msgid "remove a view"
msgstr ""

#: xml/ref/drop_view.xml:17(replaceable) xml/ref/drop_view.xml:40(replaceable)
msgid "name"
msgstr ""

#: xml/ref/drop_view.xml:16(synopsis)
#, no-wrap
msgid "DROP VIEW [ IF EXISTS ] \n    <placeholder-1/>[, ...] [\n    CASCADE | RESTRICT ]"
msgstr ""

#: xml/ref/drop_view.xml:21(title)
msgid "Description"
msgstr ""

#: xml/ref/drop_view.xml:22(para)
msgid "<command>DROP VIEW</command>drops an existing view. To execute this command you must be the owner of the view."
msgstr ""

#: xml/ref/drop_view.xml:27(title)
msgid "Parameters"
msgstr ""

#: xml/ref/drop_view.xml:31(literal)
msgid "IF EXISTS"
msgstr ""

#: xml/ref/drop_view.xml:34(para)
msgid "Do not throw an error if the view does not exist. A notice is issued in this case."
msgstr ""

#: xml/ref/drop_view.xml:43(para)
msgid "The name (optionally schema-qualified) of the view to remove."
msgstr ""

#: xml/ref/drop_view.xml:49(literal)
msgid "CASCADE"
msgstr ""

#: xml/ref/drop_view.xml:52(para)
msgid "Automatically drop objects that depend on the view (such as other views)."
msgstr ""

#: xml/ref/drop_view.xml:58(literal)
msgid "RESTRICT"
msgstr ""

#: xml/ref/drop_view.xml:61(para)
msgid "Refuse to drop the view if any objects depend on it. This is the default."
msgstr ""

#: xml/ref/drop_view.xml:68(title)
msgid "Examples"
msgstr ""

#: xml/ref/drop_view.xml:71(programlisting)
#, no-wrap
msgid "DROP VIEW kinds;"
msgstr ""

#: xml/ref/drop_view.xml:69(para)
msgid "This command will remove the view called <literal>kinds</literal>: <placeholder-1/>"
msgstr ""

#: xml/ref/drop_view.xml:74(title)
msgid "Compatibility"
msgstr ""

#: xml/ref/drop_view.xml:75(para)
msgid "This command conforms to the SQL standard, except that the standard only allows one view to be dropped per command, and apart from the <literal>IF EXISTS</literal>option, which is a <productname>PostgreSQL</productname>extension."
msgstr ""

