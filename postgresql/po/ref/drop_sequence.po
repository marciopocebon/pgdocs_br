#: xml/ref/drop_sequence.xml:21(title)
msgid "Description"
msgstr ""

#: xml/ref/drop_sequence.xml:22(para)
msgid "<command>DROP SEQUENCE</command>removes sequence number generators. A sequence can only be dropped by its owner or a superuser."
msgstr ""

#: xml/ref/drop_sequence.xml:28(title)
msgid "Parameters"
msgstr ""

#: xml/ref/drop_sequence.xml:32(literal)
msgid "IF EXISTS"
msgstr ""

#: xml/ref/drop_sequence.xml:35(para)
msgid "Do not throw an error if the sequence does not exist. A notice is issued in this case."
msgstr ""

#: xml/ref/drop_sequence.xml:44(para)
msgid "The name (optionally schema-qualified) of a sequence."
msgstr ""

#: xml/ref/drop_sequence.xml:50(literal)
msgid "CASCADE"
msgstr ""

#: xml/ref/drop_sequence.xml:53(para)
msgid "Automatically drop objects that depend on the sequence."
msgstr ""

#: xml/ref/drop_sequence.xml:59(literal)
msgid "RESTRICT"
msgstr ""

#: xml/ref/drop_sequence.xml:62(para)
msgid "Refuse to drop the sequence if any objects depend on it. This is the default."
msgstr ""

#: xml/ref/drop_sequence.xml:69(title)
msgid "Examples"
msgstr ""

#: xml/ref/drop_sequence.xml:72(programlisting)
#, no-wrap
msgid "DROP SEQUENCE serial;"
msgstr ""

#: xml/ref/drop_sequence.xml:70(para)
msgid "To remove the sequence <literal>serial</literal>: <placeholder-1/>"
msgstr ""

#: xml/ref/drop_sequence.xml:75(title)
msgid "Compatibility"
msgstr ""

#: xml/ref/drop_sequence.xml:76(para)
msgid "<command>DROP SEQUENCE</command>conforms to the <acronym>SQL</acronym>standard, except that the standard only allows one sequence to be dropped per command, and apart from the <literal>IF EXISTS</literal>option, which is a <productname>PostgreSQL</productname>extension."
msgstr ""

