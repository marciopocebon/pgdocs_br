#: xml/ref/refresh_materialized_view.xml:4(primary) xml/ref/refresh_materialized_view.xml:7(refentrytitle) xml/ref/refresh_materialized_view.xml:12(refname)
msgid "REFRESH MATERIALIZED VIEW"
msgstr ""

#: xml/ref/refresh_materialized_view.xml:8(manvolnum)
msgid "7"
msgstr ""

#: xml/ref/refresh_materialized_view.xml:9(refmiscinfo)
msgid "SQL - Language Statements"
msgstr ""

#: xml/ref/refresh_materialized_view.xml:13(refpurpose)
msgid "replace the contents of a materialized view"
msgstr ""

#: xml/ref/refresh_materialized_view.xml:18(replaceable) xml/ref/refresh_materialized_view.xml:68(replaceable)
msgid "name"
msgstr ""

#: xml/ref/refresh_materialized_view.xml:17(synopsis)
#, no-wrap
msgid "REFRESH MATERIALIZED VIEW [ CONCURRENTLY ] \n    <placeholder-1/>[ WITH [ NO ]\n    DATA ]"
msgstr ""

#: xml/ref/refresh_materialized_view.xml:22(title)
msgid "Description"
msgstr ""

#: xml/ref/refresh_materialized_view.xml:23(para)
msgid "<command>REFRESH MATERIALIZED VIEW</command>completely replaces the contents of a materialized view. The old contents are discarded. If <literal>WITH DATA</literal>is specified (or defaults) the backing query is executed to provide the new data, and the materialized view is left in a scannable state. If <literal>WITH NO DATA</literal>is specified no new data is generated and the materialized view is left in an unscannable state."
msgstr ""

#: xml/ref/refresh_materialized_view.xml:33(para)
msgid "<literal>CONCURRENTLY</literal>and <literal>WITH NO DATA</literal>may not be specified together."
msgstr ""

#: xml/ref/refresh_materialized_view.xml:39(title)
msgid "Parameters"
msgstr ""

#: xml/ref/refresh_materialized_view.xml:43(literal)
msgid "CONCURRENTLY"
msgstr ""

#: xml/ref/refresh_materialized_view.xml:46(para)
msgid "Refresh the materialized view without locking out concurrent selects on the materialized view. Without this option a refresh which affects a lot of rows will tend to use fewer resources and complete more quickly, but could block other connections which are trying to read from the materialized view. This option may be faster in cases where a small number of rows are affected."
msgstr ""

#: xml/ref/refresh_materialized_view.xml:53(para)
msgid "This option is only allowed if there is at least one <literal>UNIQUE</literal>index on the materialized view which uses only column names and includes all rows; that is, it must not index on any expressions nor include a <literal>WHERE</literal>clause."
msgstr ""

#: xml/ref/refresh_materialized_view.xml:59(para)
msgid "This option may not be used when the materialized view is not already populated."
msgstr ""

#: xml/ref/refresh_materialized_view.xml:61(para)
msgid "Even with this option only one <literal>REFRESH</literal>at a time may run against any one materialized view."
msgstr ""

#: xml/ref/refresh_materialized_view.xml:71(para)
msgid "The name (optionally schema-qualified) of the materialized view to refresh."
msgstr ""

#: xml/ref/refresh_materialized_view.xml:78(title)
msgid "Notes"
msgstr ""

#: xml/ref/refresh_materialized_view.xml:79(para)
msgid "While the default index for future <xref linkend=\"SQL-CLUSTER\">operations is retained, <command>REFRESH MATERIALIZED VIEW</command>does not order the generated rows based on this property. If you want the data to be ordered upon generation, you must use an <literal>ORDER BY</literal>clause in the backing query.</xref>"
msgstr ""

#: xml/ref/refresh_materialized_view.xml:88(title)
msgid "Examples"
msgstr ""

#: xml/ref/refresh_materialized_view.xml:94(programlisting)
#, no-wrap
msgid "REFRESH MATERIALIZED VIEW\n    order_summary;"
msgstr ""

#: xml/ref/refresh_materialized_view.xml:89(para)
msgid "This command will replace the contents of the materialized view called <literal>order_summary</literal>using the query from the materialized view's definition, and leave it in a scannable state: <placeholder-1/>"
msgstr ""

#: xml/ref/refresh_materialized_view.xml:100(programlisting)
#, no-wrap
msgid "REFRESH MATERIALIZED VIEW\n    annual_statistics_basis WITH NO DATA;"
msgstr ""

#: xml/ref/refresh_materialized_view.xml:96(para)
msgid "This command will free storage associated with the materialized view <literal>annual_statistics_basis</literal>and leave it in an unscannable state: <placeholder-1/>"
msgstr ""

#: xml/ref/refresh_materialized_view.xml:104(title)
msgid "Compatibility"
msgstr ""

#: xml/ref/refresh_materialized_view.xml:105(para)
msgid "<command>REFRESH MATERIALIZED VIEW</command>is a <productname>PostgreSQL</productname>extension."
msgstr ""

