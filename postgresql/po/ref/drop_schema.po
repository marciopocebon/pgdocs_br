#: xml/ref/drop_schema.xml:4(primary) xml/ref/drop_schema.xml:7(refentrytitle) xml/ref/drop_schema.xml:12(refname)
msgid "DROP SCHEMA"
msgstr ""

#: xml/ref/drop_schema.xml:8(manvolnum)
msgid "7"
msgstr ""

#: xml/ref/drop_schema.xml:9(refmiscinfo)
msgid "SQL - Language Statements"
msgstr ""

#: xml/ref/drop_schema.xml:13(refpurpose)
msgid "remove a schema"
msgstr ""

#: xml/ref/drop_schema.xml:17(replaceable) xml/ref/drop_schema.xml:44(replaceable)
msgid "name"
msgstr ""

#: xml/ref/drop_schema.xml:16(synopsis)
#, no-wrap
msgid "DROP SCHEMA [ IF EXISTS ] \n    <placeholder-1/>[, ...] [\n    CASCADE | RESTRICT ]"
msgstr ""

#: xml/ref/drop_schema.xml:21(title)
msgid "Description"
msgstr ""

#: xml/ref/drop_schema.xml:22(para)
msgid "<command>DROP SCHEMA</command>removes schemas from the database."
msgstr ""

#: xml/ref/drop_schema.xml:25(para)
msgid "A schema can only be dropped by its owner or a superuser. Note that the owner can drop the schema (and thereby all contained objects) even if he does not own some of the objects within the schema."
msgstr ""

#: xml/ref/drop_schema.xml:31(title)
msgid "Parameters"
msgstr ""

#: xml/ref/drop_schema.xml:35(literal)
msgid "IF EXISTS"
msgstr ""

#: xml/ref/drop_schema.xml:38(para)
msgid "Do not throw an error if the schema does not exist. A notice is issued in this case."
msgstr ""

#: xml/ref/drop_schema.xml:47(para)
msgid "The name of a schema."
msgstr ""

#: xml/ref/drop_schema.xml:52(literal)
msgid "CASCADE"
msgstr ""

#: xml/ref/drop_schema.xml:55(para)
msgid "Automatically drop objects (tables, functions, etc.) that are contained in the schema."
msgstr ""

#: xml/ref/drop_schema.xml:61(literal)
msgid "RESTRICT"
msgstr ""

#: xml/ref/drop_schema.xml:64(para)
msgid "Refuse to drop the schema if it contains any objects. This is the default."
msgstr ""

#: xml/ref/drop_schema.xml:71(title)
msgid "Examples"
msgstr ""

#: xml/ref/drop_schema.xml:75(programlisting)
#, no-wrap
msgid "DROP SCHEMA mystuff\n    CASCADE;"
msgstr ""

#: xml/ref/drop_schema.xml:72(para)
msgid "To remove schema <literal>mystuff</literal>from the database, along with everything it contains: <placeholder-1/>"
msgstr ""

#: xml/ref/drop_schema.xml:79(title)
msgid "Compatibility"
msgstr ""

#: xml/ref/drop_schema.xml:80(para)
msgid "<command>DROP SCHEMA</command>is fully conforming with the SQL standard, except that the standard only allows one schema to be dropped per command, and apart from the <literal>IF EXISTS</literal>option, which is a <productname>PostgreSQL</productname>extension."
msgstr ""

