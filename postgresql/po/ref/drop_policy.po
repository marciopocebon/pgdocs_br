msgid "DROP POLICY [ IF EXISTS ] \n    <placeholder-1/>ON \n    <placeholder-2/>"
msgstr ""

#: xml/ref/drop_policy.xml:23(title)
msgid "Description"
msgstr ""

#: xml/ref/drop_policy.xml:24(para)
msgid "<command>DROP POLICY</command>removes the specified policy from the table. Note that if the last policy is removed for a table and the table still has row level security enabled via <command>ALTER TABLE</command>, then the default-deny policy will be used. <literal>ALTER TABLE ... DISABLE ROW LEVEL SECURITY</literal>can be used to disable row level security for a table, whether policies for the table exist or not."
msgstr ""

#: xml/ref/drop_policy.xml:35(title)
msgid "Parameters"
msgstr ""

#: xml/ref/drop_policy.xml:39(literal)
msgid "IF EXISTS"
msgstr ""

#: xml/ref/drop_policy.xml:42(para)
msgid "Do not throw an error if the policy does not exist. A notice is issued in this case."
msgstr ""

#: xml/ref/drop_policy.xml:51(para)
msgid "The name of the policy to drop."
msgstr ""

#: xml/ref/drop_policy.xml:59(para)
msgid "The name (optionally schema-qualified) of the table that the policy is on."
msgstr ""

#: xml/ref/drop_policy.xml:66(title)
msgid "Examples"
msgstr ""

#: xml/ref/drop_policy.xml:70(programlisting)
#, no-wrap
msgid "DROP POLICY p1 ON\n    my_table;"
msgstr ""

#: xml/ref/drop_policy.xml:67(para)
msgid "To drop the policy called <literal>p1</literal>on the table named <literal>my_table</literal>: <placeholder-1/>"
msgstr ""

