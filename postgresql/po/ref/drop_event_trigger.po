#: xml/ref/drop_event_trigger.xml:21(title)
msgid "Description"
msgstr ""

#: xml/ref/drop_event_trigger.xml:22(para)
msgid "<command>DROP EVENT TRIGGER</command>removes an existing event trigger. To execute this command, the current user must be the owner of the event trigger."
msgstr ""

#: xml/ref/drop_event_trigger.xml:28(title)
msgid "Parameters"
msgstr ""

#: xml/ref/drop_event_trigger.xml:32(literal)
msgid "IF EXISTS"
msgstr ""

#: xml/ref/drop_event_trigger.xml:35(para)
msgid "Do not throw an error if the event trigger does not exist. A notice is issued in this case."
msgstr ""

#: xml/ref/drop_event_trigger.xml:44(para)
msgid "The name of the event trigger to remove."
msgstr ""

#: xml/ref/drop_event_trigger.xml:49(literal)
msgid "CASCADE"
msgstr ""

#: xml/ref/drop_event_trigger.xml:52(para)
msgid "Automatically drop objects that depend on the trigger."
msgstr ""

#: xml/ref/drop_event_trigger.xml:58(literal)
msgid "RESTRICT"
msgstr ""

#: xml/ref/drop_event_trigger.xml:61(para)
msgid "Refuse to drop the trigger if any objects depend on it. This is the default."
msgstr ""

#: xml/ref/drop_event_trigger.xml:68(title)
msgid "Examples"
msgstr ""

#: xml/ref/drop_event_trigger.xml:71(programlisting)
#, no-wrap
msgid "DROP EVENT TRIGGER\n    snitch;"
msgstr ""

#: xml/ref/drop_event_trigger.xml:69(para)
