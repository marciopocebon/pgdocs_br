#: xml/ref/drop_operator.xml:4(primary) xml/ref/drop_operator.xml:7(refentrytitle) xml/ref/drop_operator.xml:12(refname)
msgid "DROP OPERATOR"
msgstr ""

#: xml/ref/drop_operator.xml:8(manvolnum)
msgid "7"
msgstr ""

#: xml/ref/drop_operator.xml:9(refmiscinfo)
msgid "SQL - Language Statements"
msgstr ""

#: xml/ref/drop_operator.xml:13(refpurpose)
msgid "remove an operator"
msgstr ""

#: xml/ref/drop_operator.xml:17(replaceable) xml/ref/drop_operator.xml:44(replaceable)
msgid "name"
msgstr ""

#: xml/ref/drop_operator.xml:18(replaceable) xml/ref/drop_operator.xml:53(replaceable)
msgid "left_type"
msgstr ""

#: xml/ref/drop_operator.xml:20(replaceable) xml/ref/drop_operator.xml:64(replaceable)
msgid "right_type"
msgstr ""

#: xml/ref/drop_operator.xml:16(synopsis)
#, no-wrap
msgid "DROP OPERATOR [ IF EXISTS ] \n    <placeholder-1/>( { \n    <placeholder-2/>| NONE }\n    , { \n    <placeholder-3/>| NONE }\n    ) [ CASCADE | RESTRICT ]"
msgstr ""

#: xml/ref/drop_operator.xml:24(title)
msgid "Description"
msgstr ""

#: xml/ref/drop_operator.xml:25(para)
msgid "<command>DROP OPERATOR</command>drops an existing operator from the database system. To execute this command you must be the owner of the operator."
msgstr ""

#: xml/ref/drop_operator.xml:31(title)
msgid "Parameters"
msgstr ""

#: xml/ref/drop_operator.xml:35(literal)
msgid "IF EXISTS"
msgstr ""

#: xml/ref/drop_operator.xml:38(para)
msgid "Do not throw an error if the operator does not exist. A notice is issued in this case."
msgstr ""

#: xml/ref/drop_operator.xml:47(para)
msgid "The name (optionally schema-qualified) of an existing operator."
msgstr ""

#: xml/ref/drop_operator.xml:56(para)
msgid "The data type of the operator's left operand; write <literal>NONE</literal>if the operator has no left operand."
msgstr ""

#: xml/ref/drop_operator.xml:67(para)
msgid "The data type of the operator's right operand; write <literal>NONE</literal>if the operator has no right operand."
msgstr ""

#: xml/ref/drop_operator.xml:75(literal)
msgid "CASCADE"
msgstr ""

#: xml/ref/drop_operator.xml:78(para)
msgid "Automatically drop objects that depend on the operator."
msgstr ""

#: xml/ref/drop_operator.xml:84(literal)
msgid "RESTRICT"
msgstr ""

#: xml/ref/drop_operator.xml:87(para)
msgid "Refuse to drop the operator if any objects depend on it. This is the default."
msgstr ""

#: xml/ref/drop_operator.xml:94(title)
msgid "Examples"
msgstr ""

#: xml/ref/drop_operator.xml:98(programlisting)
#, no-wrap
msgid "DROP OPERATOR ^ (integer,\n    integer);"
msgstr ""

#: xml/ref/drop_operator.xml:95(para)
msgid "Remove the power operator <literal>a^b</literal>for type <type>integer</type>: <placeholder-1/>"
msgstr ""

#: xml/ref/drop_operator.xml:103(programlisting)
#, no-wrap
msgid "DROP OPERATOR ~ (none,\n    bit);"
msgstr ""

#: xml/ref/drop_operator.xml:100(para)
msgid "Remove the left unary bitwise complement operator <literal>~b</literal>for type <type>bit</type>: <placeholder-1/>"
msgstr ""

#: xml/ref/drop_operator.xml:108(programlisting)
#, no-wrap
msgid "DROP OPERATOR ! (bigint,\n    none);"
msgstr ""

#: xml/ref/drop_operator.xml:105(para)
msgid "Remove the right unary factorial operator <literal>x!</literal>for type <type>bigint</type>: <placeholder-1/>"
msgstr ""

#: xml/ref/drop_operator.xml:112(title)
msgid "Compatibility"
msgstr ""

#: xml/ref/drop_operator.xml:113(para)
msgid "There is no <command>DROP OPERATOR</command>statement in the SQL standard."
msgstr ""

