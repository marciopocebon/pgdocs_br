#: xml/ref/drop_user_mapping.xml:4(primary) xml/ref/drop_user_mapping.xml:7(refentrytitle) xml/ref/drop_user_mapping.xml:12(refname)
msgid "DROP USER MAPPING"
msgstr ""

#: xml/ref/drop_user_mapping.xml:8(manvolnum)
msgid "7"
msgstr ""

#: xml/ref/drop_user_mapping.xml:9(refmiscinfo)
msgid "SQL - Language Statements"
msgstr ""

#: xml/ref/drop_user_mapping.xml:13(refpurpose)
msgid "remove a user mapping for a foreign server"
msgstr ""

#: xml/ref/drop_user_mapping.xml:18(replaceable) xml/ref/drop_user_mapping.xml:48(replaceable)
msgid "user_name"
msgstr ""

#: xml/ref/drop_user_mapping.xml:20(replaceable) xml/ref/drop_user_mapping.xml:61(replaceable)
msgid "server_name"
msgstr ""

#: xml/ref/drop_user_mapping.xml:17(synopsis)
#, no-wrap
msgid "DROP USER MAPPING [ IF EXISTS ] FOR { \n    <placeholder-1/>| USER |\n    CURRENT_USER | PUBLIC } SERVER \n    <placeholder-2/>"
msgstr ""

#: xml/ref/drop_user_mapping.xml:24(title)
msgid "Description"
msgstr ""

#: xml/ref/drop_user_mapping.xml:25(para)
msgid "<command>DROP USER MAPPING</command>removes an existing user mapping from foreign server."
msgstr ""

#: xml/ref/drop_user_mapping.xml:28(para)
msgid "The owner of a foreign server can drop user mappings for that server for any user. Also, a user can drop a user mapping for his own user name if <literal>USAGE</literal>privilege on the server has been granted to the user."
msgstr ""

#: xml/ref/drop_user_mapping.xml:35(title)
msgid "Parameters"
msgstr ""

#: xml/ref/drop_user_mapping.xml:39(literal)
msgid "IF EXISTS"
msgstr ""

#: xml/ref/drop_user_mapping.xml:42(para)
msgid "Do not throw an error if the user mapping does not exist. A notice is issued in this case."
msgstr ""

#: xml/ref/drop_user_mapping.xml:51(para)
msgid "User name of the mapping. <literal>CURRENT_USER</literal>and <literal>USER</literal>match the name of the current user. <literal>PUBLIC</literal>is used to match all present and future user names in the system."
msgstr ""

#: xml/ref/drop_user_mapping.xml:64(para)
msgid "Server name of the user mapping."
msgstr ""

#: xml/ref/drop_user_mapping.xml:70(title)
msgid "Examples"
msgstr ""

#: xml/ref/drop_user_mapping.xml:74(programlisting)
#, no-wrap
msgid "DROP USER MAPPING IF EXISTS FOR bob SERVER\n    foo;"
msgstr ""

#: xml/ref/drop_user_mapping.xml:71(para)
msgid "Drop a user mapping <literal>bob</literal>, server <literal>foo</literal>if it exists: <placeholder-1/>"
msgstr ""

#: xml/ref/drop_user_mapping.xml:78(title)
msgid "Compatibility"
msgstr ""

#: xml/ref/drop_user_mapping.xml:79(para)
msgid "<command>DROP USER MAPPING</command>conforms to ISO/IEC 9075-9 (SQL/MED). The <literal>IF EXISTS</literal>clause is a <productname>PostgreSQL</productname>extension."
msgstr ""

