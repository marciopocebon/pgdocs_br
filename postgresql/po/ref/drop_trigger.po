#: xml/ref/drop_trigger.xml:4(primary) xml/ref/drop_trigger.xml:7(refentrytitle) xml/ref/drop_trigger.xml:12(refname)
msgid "DROP TRIGGER"
msgstr ""

#: xml/ref/drop_trigger.xml:8(manvolnum)
msgid "7"
msgstr ""

#: xml/ref/drop_trigger.xml:9(refmiscinfo)
msgid "SQL - Language Statements"
msgstr ""

#: xml/ref/drop_trigger.xml:13(refpurpose)
msgid "remove a trigger"
msgstr ""

#: xml/ref/drop_trigger.xml:17(replaceable) xml/ref/drop_trigger.xml:42(replaceable)
msgid "name"
msgstr ""

#: xml/ref/drop_trigger.xml:18(replaceable) xml/ref/drop_trigger.xml:50(replaceable)
msgid "table_name"
msgstr ""

#: xml/ref/drop_trigger.xml:16(synopsis)
#, no-wrap
msgid "DROP TRIGGER [ IF EXISTS ] \n    <placeholder-1/>ON \n    <placeholder-2/>[\n    CASCADE | RESTRICT ]"
msgstr ""

#: xml/ref/drop_trigger.xml:22(title)
msgid "Description"
msgstr ""

#: xml/ref/drop_trigger.xml:23(para)
msgid "<command>DROP TRIGGER</command>removes an existing trigger definition. To execute this command, the current user must be the owner of the table for which the trigger is defined."
msgstr ""

#: xml/ref/drop_trigger.xml:29(title)
msgid "Parameters"
msgstr ""

#: xml/ref/drop_trigger.xml:33(literal)
msgid "IF EXISTS"
msgstr ""

#: xml/ref/drop_trigger.xml:36(para)
msgid "Do not throw an error if the trigger does not exist. A notice is issued in this case."
msgstr ""

#: xml/ref/drop_trigger.xml:45(para)
msgid "The name of the trigger to remove."
msgstr ""

#: xml/ref/drop_trigger.xml:53(para)
msgid "The name (optionally schema-qualified) of the table for which the trigger is defined."
msgstr ""

#: xml/ref/drop_trigger.xml:59(literal)
msgid "CASCADE"
msgstr ""

#: xml/ref/drop_trigger.xml:62(para)
msgid "Automatically drop objects that depend on the trigger."
msgstr ""

#: xml/ref/drop_trigger.xml:68(literal)
msgid "RESTRICT"
msgstr ""

#: xml/ref/drop_trigger.xml:71(para)
msgid "Refuse to drop the trigger if any objects depend on it. This is the default."
msgstr ""

#: xml/ref/drop_trigger.xml:78(title)
msgid "Examples"
msgstr ""

#: xml/ref/drop_trigger.xml:82(programlisting)
#, no-wrap
msgid "DROP TRIGGER if_dist_exists ON\n    films;"
msgstr ""

#: xml/ref/drop_trigger.xml:79(para)
msgid "Destroy the trigger <literal>if_dist_exists</literal>on the table <literal>films</literal>: <placeholder-1/>"
msgstr ""

#: xml/ref/drop_trigger.xml:86(title)
msgid "Compatibility"
msgstr ""

#: xml/ref/drop_trigger.xml:87(para)
msgid "The <command>DROP TRIGGER</command>statement in <productname>PostgreSQL</productname>is incompatible with the SQL standard. In the SQL standard, trigger names are not local to tables, so the command is simply <literal>DROP TRIGGER <replaceable>name</replaceable></literal>."
msgstr ""

