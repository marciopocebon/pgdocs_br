#: xml/ref/drop_tsdictionary.xml:4(primary) xml/ref/drop_tsdictionary.xml:7(refentrytitle) xml/ref/drop_tsdictionary.xml:12(refname)
msgid "DROP TEXT SEARCH DICTIONARY"
msgstr ""

#: xml/ref/drop_tsdictionary.xml:8(manvolnum)
msgid "7"
msgstr ""

#: xml/ref/drop_tsdictionary.xml:9(refmiscinfo)
msgid "SQL - Language Statements"
msgstr ""

#: xml/ref/drop_tsdictionary.xml:13(refpurpose)
msgid "remove a text search dictionary"
msgstr ""

#: xml/ref/drop_tsdictionary.xml:17(replaceable) xml/ref/drop_tsdictionary.xml:41(replaceable)
msgid "name"
msgstr ""

#: xml/ref/drop_tsdictionary.xml:16(synopsis)
#, no-wrap
msgid "DROP TEXT SEARCH DICTIONARY [ IF EXISTS ] \n    <placeholder-1/>[ CASCADE |\n    RESTRICT ]"
msgstr ""

#: xml/ref/drop_tsdictionary.xml:21(title)
msgid "Description"
msgstr ""

#: xml/ref/drop_tsdictionary.xml:22(para)
msgid "<command>DROP TEXT SEARCH DICTIONARY</command>drops an existing text search dictionary. To execute this command you must be the owner of the dictionary."
msgstr ""

#: xml/ref/drop_tsdictionary.xml:28(title)
msgid "Parameters"
msgstr ""

#: xml/ref/drop_tsdictionary.xml:32(literal)
msgid "IF EXISTS"
msgstr ""

#: xml/ref/drop_tsdictionary.xml:35(para)
msgid "Do not throw an error if the text search dictionary does not exist. A notice is issued in this case."
msgstr ""

#: xml/ref/drop_tsdictionary.xml:44(para)
msgid "The name (optionally schema-qualified) of an existing text search dictionary."
msgstr ""

#: xml/ref/drop_tsdictionary.xml:50(literal)
msgid "CASCADE"
msgstr ""

#: xml/ref/drop_tsdictionary.xml:53(para)
msgid "Automatically drop objects that depend on the text search dictionary."
msgstr ""

#: xml/ref/drop_tsdictionary.xml:59(literal)
msgid "RESTRICT"
msgstr ""

#: xml/ref/drop_tsdictionary.xml:62(para)
msgid "Refuse to drop the text search dictionary if any objects depend on it. This is the default."
msgstr ""

#: xml/ref/drop_tsdictionary.xml:69(title)
msgid "Examples"
msgstr ""

#: xml/ref/drop_tsdictionary.xml:72(programlisting)
#, no-wrap
msgid "DROP TEXT SEARCH DICTIONARY\n    english;"
msgstr ""

#: xml/ref/drop_tsdictionary.xml:70(para)
msgid "Remove the text search dictionary <literal>english</literal>: <placeholder-1/>This command will not succeed if there are any existing text search configurations that use the dictionary. Add <literal>CASCADE</literal>to drop such configurations along with the dictionary."
msgstr ""

#: xml/ref/drop_tsdictionary.xml:80(title)
msgid "Compatibility"
msgstr ""

#: xml/ref/drop_tsdictionary.xml:81(para)
msgid "There is no <command>DROP TEXT SEARCH DICTIONARY</command>statement in the SQL standard."
msgstr ""

