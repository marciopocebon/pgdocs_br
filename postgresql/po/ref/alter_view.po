msgid "Notes"
msgstr ""

#: xml/ref/alter_view.xml:170(para)
msgid "For historical reasons, <command>ALTER TABLE</command>can be used with views too; but the only variants of <command>ALTER TABLE</command>that are allowed with views are equivalent to the ones shown above."
msgstr ""

#: xml/ref/alter_view.xml:177(title)
msgid "Examples"
msgstr ""

#: xml/ref/alter_view.xml:181(programlisting)
#, no-wrap
msgid "ALTER VIEW foo RENAME TO\n    bar;"
msgstr ""

#: xml/ref/alter_view.xml:178(para)
msgid "To rename the view <literal>foo</literal>to <literal>bar</literal>: <placeholder-1/>"
msgstr ""

#: xml/ref/alter_view.xml:184(programlisting)
#, no-wrap
msgid "CREATE TABLE base_table (id int, ts\n    timestamptz); CREATE VIEW a_view AS SELECT * FROM base_table;\n    ALTER VIEW a_view ALTER COLUMN ts SET DEFAULT now(); INSERT\n    INTO base_table(id) VALUES(1); -- ts will receive a NULL INSERT\n    INTO a_view(id) VALUES(2); -- ts will receive the current\n    time"
msgstr ""

