#: xml/ref/drop_foreign_table.xml:4(primary) xml/ref/drop_foreign_table.xml:7(refentrytitle) xml/ref/drop_foreign_table.xml:12(refname)
msgid "DROP FOREIGN TABLE"
msgstr ""

#: xml/ref/drop_foreign_table.xml:8(manvolnum)
msgid "7"
msgstr ""

#: xml/ref/drop_foreign_table.xml:9(refmiscinfo)
msgid "SQL - Language Statements"
msgstr ""

#: xml/ref/drop_foreign_table.xml:13(refpurpose)
msgid "remove a foreign table"
msgstr ""

#: xml/ref/drop_foreign_table.xml:17(replaceable) xml/ref/drop_foreign_table.xml:40(replaceable)
msgid "name"
msgstr ""

#: xml/ref/drop_foreign_table.xml:16(synopsis)
#, no-wrap
msgid "DROP FOREIGN TABLE [ IF EXISTS ] \n    <placeholder-1/>[, ...] [\n    CASCADE | RESTRICT ]"
msgstr ""

#: xml/ref/drop_foreign_table.xml:21(title)
msgid "Description"
msgstr ""

#: xml/ref/drop_foreign_table.xml:22(para)
msgid "<command>DROP FOREIGN TABLE</command>removes a foreign table. Only the owner of a foreign table can remove it."
msgstr ""

#: xml/ref/drop_foreign_table.xml:27(title)
msgid "Parameters"
msgstr ""

#: xml/ref/drop_foreign_table.xml:31(literal)
msgid "IF EXISTS"
msgstr ""

#: xml/ref/drop_foreign_table.xml:34(para)
msgid "Do not throw an error if the foreign table does not exist. A notice is issued in this case."
msgstr ""

#: xml/ref/drop_foreign_table.xml:43(para)
msgid "The name (optionally schema-qualified) of the foreign table to drop."
msgstr ""

#: xml/ref/drop_foreign_table.xml:49(literal)
msgid "CASCADE"
msgstr ""

#: xml/ref/drop_foreign_table.xml:52(para)
msgid "Automatically drop objects that depend on the foreign table (such as views)."
msgstr ""

#: xml/ref/drop_foreign_table.xml:58(literal)
msgid "RESTRICT"
msgstr ""

#: xml/ref/drop_foreign_table.xml:61(para)
msgid "Refuse to drop the foreign table if any objects depend on it. This is the default."
msgstr ""

#: xml/ref/drop_foreign_table.xml:68(title)
msgid "Examples"
msgstr ""

#: xml/ref/drop_foreign_table.xml:72(programlisting)
#, no-wrap
msgid "DROP FOREIGN TABLE films,\n    distributors;"
msgstr ""

#: xml/ref/drop_foreign_table.xml:69(para)
msgid "To destroy two foreign tables, <literal>films</literal>and <literal>distributors</literal>: <placeholder-1/>"
msgstr ""

#: xml/ref/drop_foreign_table.xml:76(title)
msgid "Compatibility"
msgstr ""

#: xml/ref/drop_foreign_table.xml:77(para)
msgid "This command conforms to the ISO/IEC 9075-9 (SQL/MED), except that the standard only allows one foreign table to be dropped per command, and apart from the <literal>IF EXISTS</literal>option, which is a <productname>PostgreSQL</productname>extension."
msgstr ""

