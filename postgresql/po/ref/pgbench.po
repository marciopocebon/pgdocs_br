msgstr ""

#: xml/ref/pgbench.xml:177(para)
msgid "Perform no vacuuming after initialization."
msgstr ""

#: xml/ref/pgbench.xml:182(option)
msgid "-q"
msgstr ""

#: xml/ref/pgbench.xml:185(option)
msgid "--quiet"
msgstr ""

#: xml/ref/pgbench.xml:188(para)
msgid "Switch logging to quiet mode, producing only one progress message per 5 seconds. The default logging prints one message each 100000 rows, which often outputs many lines per second (especially on good hardware)."
msgstr ""

#: xml/ref/pgbench.xml:197(option) xml/ref/pgbench.xml:551(option)
