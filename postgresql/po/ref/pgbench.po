msgid "-F"
msgstr ""

#: xml/ref/pgbench.xml:155(replaceable) xml/ref/pgbench.xml:159(replaceable)
msgid "fillfactor"
msgstr ""

#: xml/ref/pgbench.xml:158(option)
msgid "--fillfactor="
msgstr ""

#: xml/ref/pgbench.xml:162(para)
msgid "Create the <structname>pgbench_accounts</structname>, <structname>pgbench_tellers</structname>and <structname>pgbench_branches</structname>tables with the given fillfactor. Default is 100."
msgstr ""

#: xml/ref/pgbench.xml:171(option) xml/ref/pgbench.xml:429(option)
