#: xml/ref/pg_xlogdump.xml:35(title)
msgid "Description"
msgstr ""

#: xml/ref/pg_xlogdump.xml:36(para)
msgid "<command>pg_xlogdump</command>displays the write-ahead log (WAL) and is mainly useful for debugging or educational purposes."
msgstr ""

#: xml/ref/pg_xlogdump.xml:40(para)
msgid "This utility can only be run by the user who installed the server, because it requires read-only access to the data directory."
msgstr ""

#: xml/ref/pg_xlogdump.xml:45(title)
msgid "Options"
msgstr ""

#: xml/ref/pg_xlogdump.xml:54(para)
msgid "Start reading at the specified log segment file. This implicitly determines the path in which files will be searched for, and the timeline to use."
msgstr ""

#: xml/ref/pg_xlogdump.xml:64(para)
msgid "Stop after reading the specified log segment file."
msgstr ""

#: xml/ref/pg_xlogdump.xml:70(option)
msgid "-b"
msgstr ""

#: xml/ref/pg_xlogdump.xml:73(option)
msgid "--bkp-details"
msgstr ""

#: xml/ref/pg_xlogdump.xml:76(para)
msgid "Output detailed information about backup blocks."
msgstr ""

#: xml/ref/pg_xlogdump.xml:83(replaceable) xml/ref/pg_xlogdump.xml:87(replaceable)
msgid "end"
msgstr ""

#: xml/ref/pg_xlogdump.xml:82(option)
msgid "-e <placeholder-1/>"
msgstr ""

#: xml/ref/pg_xlogdump.xml:86(option)
msgid "--end= <placeholder-1/>"
msgstr ""

#: xml/ref/pg_xlogdump.xml:90(para)
msgid "Stop reading at the specified log position, instead of reading to the end of the log stream."
msgstr ""

#: xml/ref/pg_xlogdump.xml:96(option)
msgid "-f"
msgstr ""

#: xml/ref/pg_xlogdump.xml:99(option)
msgid "--follow"
msgstr ""

#: xml/ref/pg_xlogdump.xml:102(para)
msgid "After reaching the end of valid WAL, keep polling once per second for new WAL to appear."
msgstr ""

#: xml/ref/pg_xlogdump.xml:109(replaceable) xml/ref/pg_xlogdump.xml:113(replaceable)
msgid "limit"
msgstr ""

#: xml/ref/pg_xlogdump.xml:108(option)
msgid "-n <placeholder-1/>"
msgstr ""

#: xml/ref/pg_xlogdump.xml:112(option)
msgid "--limit= <placeholder-1/>"
msgstr ""

#: xml/ref/pg_xlogdump.xml:116(para)
msgid "Display the specified number of records, then stop."
msgstr ""

#: xml/ref/pg_xlogdump.xml:123(replaceable) xml/ref/pg_xlogdump.xml:127(replaceable)
msgid "path"
msgstr ""

#: xml/ref/pg_xlogdump.xml:122(option)
msgid "-p <placeholder-1/>"
msgstr ""

#: xml/ref/pg_xlogdump.xml:126(option)
msgid "--path= <placeholder-1/>"
msgstr ""

#: xml/ref/pg_xlogdump.xml:130(para)
msgid "Directory in which to find log segment files. The default is to search for them in the <literal>pg_xlog</literal>subdirectory of the current directory."
msgstr ""

