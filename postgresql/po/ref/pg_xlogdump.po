msgid "rmgr"
msgstr ""

#: xml/ref/pg_xlogdump.xml:138(option)
msgid "-r <placeholder-1/>"
msgstr ""

#: xml/ref/pg_xlogdump.xml:142(option)
msgid "--rmgr= <placeholder-1/>"
msgstr ""

#: xml/ref/pg_xlogdump.xml:146(para)
msgid "Only display records generated by the specified resource manager. If <literal>list</literal>is passed as name, print a list of valid resource manager names, and exit."
msgstr ""

#: xml/ref/pg_xlogdump.xml:155(replaceable) xml/ref/pg_xlogdump.xml:159(replaceable)
msgid "start"
msgstr ""

#: xml/ref/pg_xlogdump.xml:154(option)
msgid "-s <placeholder-1/>"
msgstr ""

#: xml/ref/pg_xlogdump.xml:158(option)
msgid "--start= <placeholder-1/>"
msgstr ""

#: xml/ref/pg_xlogdump.xml:162(para)
msgid "Log position at which to start reading. The default is to start reading the first valid log record found in the earliest file found."
msgstr ""

