msgid "timeline"
msgstr ""

#: xml/ref/pg_xlogdump.xml:169(option)
msgid "-t <placeholder-1/>"
msgstr ""

#: xml/ref/pg_xlogdump.xml:173(option)
msgid "--timelime= <placeholder-1/>"
msgstr ""

#: xml/ref/pg_xlogdump.xml:177(para)
msgid "Timeline from which to read log records. The default is to use the value in <literal>startseg</literal>, if that is specified; otherwise, the default is 1."
msgstr ""

#: xml/ref/pg_xlogdump.xml:185(option)
msgid "-V"
msgstr ""

#: xml/ref/pg_xlogdump.xml:188(option)
msgid "--version"
msgstr ""

#: xml/ref/pg_xlogdump.xml:191(para)
msgid "Print the <application>pg_xlogdump</application>version and exit."
msgstr ""

#: xml/ref/pg_xlogdump.xml:199(replaceable) xml/ref/pg_xlogdump.xml:203(replaceable)
msgid "xid"
msgstr ""

#: xml/ref/pg_xlogdump.xml:198(option)
msgid "-x <placeholder-1/>"
msgstr ""

#: xml/ref/pg_xlogdump.xml:202(option)
msgid "--xid= <placeholder-1/>"
msgstr ""

#: xml/ref/pg_xlogdump.xml:206(para)
msgid "Only display records marked with the given transaction ID."
msgstr ""

#: xml/ref/pg_xlogdump.xml:212(option)
msgid "-z"
msgstr ""

#: xml/ref/pg_xlogdump.xml:215(option)
msgid "--stats[=record]"
msgstr ""

#: xml/ref/pg_xlogdump.xml:218(para)
msgid "Display summary statistics (number and size of records and full-page images) instead of individual records. Optionally generate statistics per-record instead of per-rmgr."
msgstr ""

#: xml/ref/pg_xlogdump.xml:226(option)
msgid "-?"
msgstr ""

#: xml/ref/pg_xlogdump.xml:229(option)
msgid "--help"
msgstr ""

#: xml/ref/pg_xlogdump.xml:232(para)
msgid "Show help about <application>pg_xlogdump</application>command line arguments, and exit."
msgstr ""

#: xml/ref/pg_xlogdump.xml:46(para)
msgid "The following command-line options control the location and format of the output: <placeholder-1/>"
msgstr ""

#: xml/ref/pg_xlogdump.xml:240(title)
msgid "Notes"
msgstr ""

#: xml/ref/pg_xlogdump.xml:241(para)
msgid "Can give wrong results when the server is running."
msgstr ""

#: xml/ref/pg_xlogdump.xml:242(para)
msgid "Only the specified timeline is displayed (or the default, if none is specified). Records in other timelines are ignored."
msgstr ""

#: xml/ref/pg_xlogdump.xml:245(para)
msgid "<application>pg_xlogdump</application>cannot read WAL files with suffix <literal>.partial</literal>. If those files need to be read, <literal>.partial</literal>suffix needs to be removed from the file name."
msgstr ""

