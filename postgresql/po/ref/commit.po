#: xml/ref/commit.xml:19(title)
msgid "Description"
msgstr ""

#: xml/ref/commit.xml:20(para)
msgid "<command>COMMIT</command>commits the current transaction. All changes made by the transaction become visible to others and are guaranteed to be durable if a crash occurs."
msgstr ""

#: xml/ref/commit.xml:26(title)
msgid "Parameters"
msgstr ""

#: xml/ref/commit.xml:30(literal)
msgid "WORK"
msgstr ""

#: xml/ref/commit.xml:33(literal)
msgid "TRANSACTION"
msgstr ""

#: xml/ref/commit.xml:36(para)
msgid "Optional key words. They have no effect."
msgstr ""

#: xml/ref/commit.xml:42(title)
msgid "Notes"
msgstr ""

#: xml/ref/commit.xml:43(para)
msgid "Use <xref linkend=\"SQL-ROLLBACK\">to abort a transaction.</xref>"
msgstr ""

#: xml/ref/commit.xml:46(para)
msgid "Issuing <command>COMMIT</command>when not inside a transaction does no harm, but it will provoke a warning message."
msgstr ""

#: xml/ref/commit.xml:51(title)
msgid "Examples"
msgstr ""

