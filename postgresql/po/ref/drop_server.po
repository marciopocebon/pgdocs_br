msgid "DROP SERVER"
msgstr ""

#: xml/ref/drop_server.xml:8(manvolnum)
msgid "7"
msgstr ""

#: xml/ref/drop_server.xml:9(refmiscinfo)
msgid "SQL - Language Statements"
msgstr ""

#: xml/ref/drop_server.xml:13(refpurpose)
msgid "remove a foreign server descriptor"
msgstr ""

#: xml/ref/drop_server.xml:17(replaceable) xml/ref/drop_server.xml:41(replaceable)
msgid "name"
msgstr ""

#: xml/ref/drop_server.xml:16(synopsis)
#, no-wrap
msgid "DROP SERVER [ IF EXISTS ] \n    <placeholder-1/>[ CASCADE |\n    RESTRICT ]"
msgstr ""

#: xml/ref/drop_server.xml:21(title)
msgid "Description"
msgstr ""

#: xml/ref/drop_server.xml:22(para)
msgid "<command>DROP SERVER</command>removes an existing foreign server descriptor. To execute this command, the current user must be the owner of the server."
msgstr ""

#: xml/ref/drop_server.xml:28(title)
msgid "Parameters"
msgstr ""

#: xml/ref/drop_server.xml:32(literal)
msgid "IF EXISTS"
msgstr ""

#: xml/ref/drop_server.xml:35(para)
msgid "Do not throw an error if the server does not exist. A notice is issued in this case."
msgstr ""

#: xml/ref/drop_server.xml:44(para)
msgid "The name of an existing server."
msgstr ""

#: xml/ref/drop_server.xml:49(literal)
msgid "CASCADE"
msgstr ""

#: xml/ref/drop_server.xml:52(para)
msgid "Automatically drop objects that depend on the server (such as user mappings)."
msgstr ""

#: xml/ref/drop_server.xml:58(literal)
msgid "RESTRICT"
msgstr ""

#: xml/ref/drop_server.xml:61(para)
msgid "Refuse to drop the server if any objects depend on it. This is the default."
msgstr ""

#: xml/ref/drop_server.xml:68(title)
msgid "Examples"
msgstr ""

#: xml/ref/drop_server.xml:71(programlisting)
#, no-wrap
msgid "DROP SERVER IF EXISTS\n    foo;"
msgstr ""

#: xml/ref/drop_server.xml:69(para)
msgid "Drop a server <literal>foo</literal>if it exists: <placeholder-1/>"
msgstr ""

#: xml/ref/drop_server.xml:75(title)
msgid "Compatibility"
msgstr ""

#: xml/ref/drop_server.xml:76(para)
msgid "<command>DROP SERVER</command>conforms to ISO/IEC 9075-9 (SQL/MED). The <literal>IF EXISTS</literal>clause is a <productname>PostgreSQL</productname>extension."
msgstr ""

