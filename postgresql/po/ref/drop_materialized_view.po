#: xml/ref/drop_materialized_view.xml:4(primary) xml/ref/drop_materialized_view.xml:7(refentrytitle) xml/ref/drop_materialized_view.xml:12(refname)
msgid "DROP MATERIALIZED VIEW"
msgstr ""

#: xml/ref/drop_materialized_view.xml:8(manvolnum)
msgid "7"
msgstr ""

#: xml/ref/drop_materialized_view.xml:9(refmiscinfo)
msgid "SQL - Language Statements"
msgstr ""

#: xml/ref/drop_materialized_view.xml:13(refpurpose)
msgid "remove a materialized view"
msgstr ""

#: xml/ref/drop_materialized_view.xml:17(replaceable) xml/ref/drop_materialized_view.xml:41(replaceable)
msgid "name"
msgstr ""

#: xml/ref/drop_materialized_view.xml:16(synopsis)
#, no-wrap
msgid "DROP MATERIALIZED VIEW [ IF EXISTS ] \n    <placeholder-1/>[, ...] [\n    CASCADE | RESTRICT ]"
msgstr ""

#: xml/ref/drop_materialized_view.xml:21(title)
msgid "Description"
msgstr ""

#: xml/ref/drop_materialized_view.xml:22(para)
msgid "<command>DROP MATERIALIZED VIEW</command>drops an existing materialized view. To execute this command you must be the owner of the materialized view."
msgstr ""

#: xml/ref/drop_materialized_view.xml:28(title)
msgid "Parameters"
msgstr ""

#: xml/ref/drop_materialized_view.xml:32(literal)
msgid "IF EXISTS"
msgstr ""

#: xml/ref/drop_materialized_view.xml:35(para)
msgid "Do not throw an error if the materialized view does not exist. A notice is issued in this case."
msgstr ""

#: xml/ref/drop_materialized_view.xml:44(para)
msgid "The name (optionally schema-qualified) of the materialized view to remove."
msgstr ""

#: xml/ref/drop_materialized_view.xml:50(literal)
msgid "CASCADE"
msgstr ""

#: xml/ref/drop_materialized_view.xml:53(para)
msgid "Automatically drop objects that depend on the materialized view (such as other materialized views, or regular views)."
msgstr ""

#: xml/ref/drop_materialized_view.xml:60(literal)
msgid "RESTRICT"
msgstr ""

#: xml/ref/drop_materialized_view.xml:63(para)
msgid "Refuse to drop the materialized view if any objects depend on it. This is the default."
msgstr ""

#: xml/ref/drop_materialized_view.xml:70(title)
msgid "Examples"
msgstr ""

#: xml/ref/drop_materialized_view.xml:73(programlisting)
#, no-wrap
msgid "DROP MATERIALIZED VIEW\n    order_summary;"
msgstr ""

#: xml/ref/drop_materialized_view.xml:71(para)
msgid "This command will remove the materialized view called <literal>order_summary</literal>: <placeholder-1/>"
msgstr ""

#: xml/ref/drop_materialized_view.xml:77(title)
msgid "Compatibility"
msgstr ""

#: xml/ref/drop_materialized_view.xml:78(para)
msgid "<command>DROP MATERIALIZED VIEW</command>is a <productname>PostgreSQL</productname>extension."
msgstr ""

