#: xml/ref/drop_index.xml:4(primary) xml/ref/drop_index.xml:7(refentrytitle) xml/ref/drop_index.xml:12(refname)
msgid "DROP INDEX"
msgstr ""

#: xml/ref/drop_index.xml:8(manvolnum)
msgid "7"
msgstr ""

#: xml/ref/drop_index.xml:9(refmiscinfo)
msgid "SQL - Language Statements"
msgstr ""

#: xml/ref/drop_index.xml:13(refpurpose)
msgid "remove an index"
msgstr ""

#: xml/ref/drop_index.xml:17(replaceable) xml/ref/drop_index.xml:67(replaceable)
msgid "name"
msgstr ""

#: xml/ref/drop_index.xml:16(synopsis)
#, no-wrap
msgid "DROP INDEX [ CONCURRENTLY ] [ IF EXISTS ] \n    <placeholder-1/>[, ...] [\n    CASCADE | RESTRICT ]"
msgstr ""

#: xml/ref/drop_index.xml:21(title)
msgid "Description"
msgstr ""

#: xml/ref/drop_index.xml:22(para)
msgid "<command>DROP INDEX</command>drops an existing index from the database system. To execute this command you must be the owner of the index."
msgstr ""

#: xml/ref/drop_index.xml:28(title)
msgid "Parameters"
msgstr ""

#: xml/ref/drop_index.xml:32(literal)
msgid "CONCURRENTLY"
msgstr ""

#: xml/ref/drop_index.xml:35(para)
msgid "Drop the index without locking out concurrent selects, inserts, updates, and deletes on the index's table. A normal <command>DROP INDEX</command>acquires exclusive lock on the table, blocking other accesses until the index drop can be completed. With this option, the command instead waits until conflicting transactions have completed."
msgstr ""

#: xml/ref/drop_index.xml:43(para)
msgid "There are several caveats to be aware of when using this option. Only one index name can be specified, and the <literal>CASCADE</literal>option is not supported. (Thus, an index that supports a <literal>UNIQUE</literal>or <literal>PRIMARY KEY</literal>constraint cannot be dropped this way.) Also, regular <command>DROP INDEX</command>commands can be performed within a transaction block, but <command>DROP INDEX CONCURRENTLY</command>cannot."
msgstr ""

#: xml/ref/drop_index.xml:58(literal)
msgid "IF EXISTS"
msgstr ""

#: xml/ref/drop_index.xml:61(para)
msgid "Do not throw an error if the index does not exist. A notice is issued in this case."
msgstr ""

#: xml/ref/drop_index.xml:70(para)
msgid "The name (optionally schema-qualified) of an index to remove."
msgstr ""

#: xml/ref/drop_index.xml:76(literal)
msgid "CASCADE"
msgstr ""

#: xml/ref/drop_index.xml:79(para)
msgid "Automatically drop objects that depend on the index."
msgstr ""

#: xml/ref/drop_index.xml:85(literal)
msgid "RESTRICT"
msgstr ""

#: xml/ref/drop_index.xml:88(para)
msgid "Refuse to drop the index if any objects depend on it. This is the default."
msgstr ""

#: xml/ref/drop_index.xml:95(title)
msgid "Examples"
msgstr ""

#: xml/ref/drop_index.xml:98(programlisting)
#, no-wrap
msgid "DROP INDEX title_idx;"
msgstr ""

#: xml/ref/drop_index.xml:96(para)
msgid "This command will remove the index <literal>title_idx</literal>: <placeholder-1/>"
msgstr ""

#: xml/ref/drop_index.xml:101(title)
msgid "Compatibility"
msgstr ""

#: xml/ref/drop_index.xml:102(para)
msgid "<command>DROP INDEX</command>is a <productname>PostgreSQL</productname>language extension. There are no provisions for indexes in the SQL standard."
msgstr ""

