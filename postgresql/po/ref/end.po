msgstr ""

#: xml/ref/end.xml:47(para)
msgid "Use <xref linkend=\"SQL-ROLLBACK\">to abort a transaction.</xref>"
msgstr ""

#: xml/ref/end.xml:50(para)
msgid "Issuing <command>END</command>when not inside a transaction does no harm, but it will provoke a warning message."
msgstr ""

