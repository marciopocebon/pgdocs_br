#: xml/ref/drop_tsconfig.xml:4(primary) xml/ref/drop_tsconfig.xml:7(refentrytitle) xml/ref/drop_tsconfig.xml:12(refname)
msgid "DROP TEXT SEARCH CONFIGURATION"
msgstr ""

#: xml/ref/drop_tsconfig.xml:8(manvolnum)
msgid "7"
msgstr ""

#: xml/ref/drop_tsconfig.xml:9(refmiscinfo)
msgid "SQL - Language Statements"
msgstr ""

#: xml/ref/drop_tsconfig.xml:13(refpurpose)
msgid "remove a text search configuration"
msgstr ""

#: xml/ref/drop_tsconfig.xml:17(replaceable) xml/ref/drop_tsconfig.xml:42(replaceable)
msgid "name"
msgstr ""

#: xml/ref/drop_tsconfig.xml:16(synopsis)
#, no-wrap
msgid "DROP TEXT SEARCH CONFIGURATION [ IF EXISTS ] \n    <placeholder-1/>[ CASCADE |\n    RESTRICT ]"
msgstr ""

#: xml/ref/drop_tsconfig.xml:21(title)
msgid "Description"
msgstr ""

#: xml/ref/drop_tsconfig.xml:22(para)
msgid "<command>DROP TEXT SEARCH CONFIGURATION</command>drops an existing text search configuration. To execute this command you must be the owner of the configuration."
msgstr ""

#: xml/ref/drop_tsconfig.xml:28(title)
msgid "Parameters"
msgstr ""

#: xml/ref/drop_tsconfig.xml:32(literal)
msgid "IF EXISTS"
msgstr ""

#: xml/ref/drop_tsconfig.xml:35(para)
msgid "Do not throw an error if the text search configuration does not exist. A notice is issued in this case."
msgstr ""

#: xml/ref/drop_tsconfig.xml:45(para)
msgid "The name (optionally schema-qualified) of an existing text search configuration."
msgstr ""

#: xml/ref/drop_tsconfig.xml:51(literal)
msgid "CASCADE"
msgstr ""

#: xml/ref/drop_tsconfig.xml:54(para)
msgid "Automatically drop objects that depend on the text search configuration."
msgstr ""

#: xml/ref/drop_tsconfig.xml:60(literal)
msgid "RESTRICT"
msgstr ""

#: xml/ref/drop_tsconfig.xml:63(para)
msgid "Refuse to drop the text search configuration if any objects depend on it. This is the default."
msgstr ""

#: xml/ref/drop_tsconfig.xml:70(title)
msgid "Examples"
msgstr ""

#: xml/ref/drop_tsconfig.xml:73(programlisting)
#, no-wrap
msgid "DROP TEXT SEARCH CONFIGURATION\n    my_english;"
msgstr ""

#: xml/ref/drop_tsconfig.xml:71(para)
msgid "Remove the text search configuration <literal>my_english</literal>: <placeholder-1/>This command will not succeed if there are any existing indexes that reference the configuration in <function>to_tsvector</function>calls. Add <literal>CASCADE</literal>to drop such indexes along with the text search configuration."
msgstr ""

#: xml/ref/drop_tsconfig.xml:82(title)
msgid "Compatibility"
msgstr ""

#: xml/ref/drop_tsconfig.xml:83(para)
msgid "There is no <command>DROP TEXT SEARCH CONFIGURATION</command>statement in the SQL standard."
msgstr ""

