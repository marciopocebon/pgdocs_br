#: xml/ref/rollback.xml:19(title)
msgid "Description"
msgstr ""

#: xml/ref/rollback.xml:20(para)
msgid "<command>ROLLBACK</command>rolls back the current transaction and causes all the updates made by the transaction to be discarded."
msgstr ""

#: xml/ref/rollback.xml:26(title)
msgid "Parameters"
msgstr ""

#: xml/ref/rollback.xml:30(literal)
msgid "WORK"
msgstr ""

#: xml/ref/rollback.xml:33(literal)
msgid "TRANSACTION"
msgstr ""

#: xml/ref/rollback.xml:36(para)
msgid "Optional key words. They have no effect."
msgstr ""

#: xml/ref/rollback.xml:42(title)
msgid "Notes"
msgstr ""

#: xml/ref/rollback.xml:43(para)
msgid "Use <xref linkend=\"SQL-COMMIT\">to successfully terminate a transaction.</xref>"
msgstr ""

#: xml/ref/rollback.xml:46(para)
msgid "Issuing <command>ROLLBACK</command>outside of a transaction block emits a warning and otherwise has no effect."
msgstr ""

#: xml/ref/rollback.xml:51(title)
msgid "Examples"
msgstr ""

#: xml/ref/rollback.xml:53(programlisting)
#, no-wrap
msgid "ROLLBACK;"
msgstr ""

#: xml/ref/rollback.xml:52(para)
