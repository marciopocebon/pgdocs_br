msgstr ""

#: xml/ref/pg_isready.xml:30(title)
msgid "Description"
msgstr ""

#: xml/ref/pg_isready.xml:31(para)
msgid "<application>pg_isready</application>is a utility for checking the connection status of a <productname>PostgreSQL</productname>database server. The exit status specifies the result of the connection check."
msgstr ""

#: xml/ref/pg_isready.xml:38(title)
msgid "Options"
msgstr ""

#: xml/ref/pg_isready.xml:43(replaceable) xml/ref/pg_isready.xml:48(replaceable)
msgid "dbname"
msgstr ""

#: xml/ref/pg_isready.xml:42(option)
msgid "-d <placeholder-1/>"
msgstr ""

#: xml/ref/pg_isready.xml:47(option)
msgid "--dbname= <placeholder-1/>"
msgstr ""

#: xml/ref/pg_isready.xml:52(para)
msgid "Specifies the name of the database to connect to."
msgstr ""

#: xml/ref/pg_isready.xml:54(para)
msgid "If this parameter contains an <symbol>=</symbol>sign or starts with a valid <acronym>URI</acronym>prefix ( <literal>postgresql://</literal>or <literal>postgres://</literal>), it is treated as a <parameter>conninfo</parameter>string. See <xref linkend=\"libpq-connstring\">for more information.</xref>"
msgstr ""

#: xml/ref/pg_isready.xml:67(replaceable) xml/ref/pg_isready.xml:72(replaceable)
msgid "hostname"
msgstr ""

#: xml/ref/pg_isready.xml:66(option)
msgid "-h <placeholder-1/>"
msgstr ""

#: xml/ref/pg_isready.xml:71(option)
msgid "--host= <placeholder-1/>"
msgstr ""

#: xml/ref/pg_isready.xml:76(para)
msgid "Specifies the host name of the machine on which the server is running. If the value begins with a slash, it is used as the directory for the Unix-domain socket."
msgstr ""

#: xml/ref/pg_isready.xml:85(replaceable) xml/ref/pg_isready.xml:90(replaceable)
msgid "port"
msgstr ""

#: xml/ref/pg_isready.xml:84(option)
msgid "-p <placeholder-1/>"
msgstr ""

#: xml/ref/pg_isready.xml:89(option)
msgid "--port= <placeholder-1/>"
msgstr ""

#: xml/ref/pg_isready.xml:94(para)
msgid "Specifies the TCP port or the local Unix-domain socket file extension on which the server is listening for connections. Defaults to the value of the <envar>PGPORT</envar>environment variable or, if not set, to the port specified at compile time, usually 5432."
msgstr ""

#: xml/ref/pg_isready.xml:104(option)
msgid "-q"
msgstr ""

#: xml/ref/pg_isready.xml:107(option)
msgid "--quiet"
msgstr ""

#: xml/ref/pg_isready.xml:110(para)
msgid "Do not display status message. This is useful when scripting."
msgstr ""

#: xml/ref/pg_isready.xml:117(replaceable) xml/ref/pg_isready.xml:122(replaceable)
msgid "seconds"
msgstr ""

#: xml/ref/pg_isready.xml:116(option)
msgid "-t <placeholder-1/>"
msgstr ""

#: xml/ref/pg_isready.xml:121(option)
msgid "--timeout= <placeholder-1/>"
msgstr ""

#: xml/ref/pg_isready.xml:126(para)
msgid "The maximum number of seconds to wait when attempting connection before returning that the server is not responding. Setting to 0 disables. The default is 3 seconds."
msgstr ""

#: xml/ref/pg_isready.xml:135(replaceable) xml/ref/pg_isready.xml:140(replaceable)
msgid "username"
msgstr ""

#: xml/ref/pg_isready.xml:134(option)
msgid "-U <placeholder-1/>"
msgstr ""

#: xml/ref/pg_isready.xml:139(option)
msgid "--username= <placeholder-1/>"
msgstr ""

#: xml/ref/pg_isready.xml:144(para)
msgid "Connect to the database as the user <replaceable class=\"parameter\"> username</replaceable>instead of the default."
msgstr ""

#: xml/ref/pg_isready.xml:151(option)
msgid "-V"
msgstr ""

#: xml/ref/pg_isready.xml:154(option)
msgid "--version"
msgstr ""

#: xml/ref/pg_isready.xml:157(para)
msgid "Print the <application>pg_isready</application>version and exit."
msgstr ""

#: xml/ref/pg_isready.xml:164(option)
msgid "-?"
msgstr ""

#: xml/ref/pg_isready.xml:167(option)
msgid "--help"
msgstr ""

#: xml/ref/pg_isready.xml:170(para)
msgid "Show help about <application>pg_isready</application>command line arguments, and exit."
msgstr ""

#: xml/ref/pg_isready.xml:178(title)
msgid "Exit Status"
msgstr ""

#: xml/ref/pg_isready.xml:179(para)
msgid "<application>pg_isready</application>returns <literal>0</literal>to the shell if the server is accepting connections normally, <literal>1</literal>if the server is rejecting connections (for example during startup), <literal>2</literal>if there was no response to the connection attempt, and <literal>3</literal>if no attempt was made (for example due to invalid parameters)."
msgstr ""

#: xml/ref/pg_isready.xml:191(title)
msgid "Environment"
msgstr ""

#: xml/ref/pg_isready.xml:192(para)
msgid "<command>pg_isready</command>, like most other <productname>PostgreSQL</productname>utilities, also uses the environment variables supported by <application>libpq</application>(see <xref linkend=\"libpq-envars\">).</xref>"
msgstr ""

#: xml/ref/pg_isready.xml:200(title)
msgid "Notes"
msgstr ""

#: xml/ref/pg_isready.xml:201(para)
msgid "It is not necessary to supply correct user name, password, or database name values to obtain the server status; however, if incorrect values are provided, the server will log a failed connection attempt."
msgstr ""

#: xml/ref/pg_isready.xml:207(title)
msgid "Examples"
msgstr ""

#: xml/ref/pg_isready.xml:208(para)
msgid "Standard Usage: <screen>\n      <prompt>$</prompt>\n      <userinput>pg_isready</userinput>\n      <computeroutput>/tmp:5432 - accepting\n      connections</computeroutput>\n      <prompt>$</prompt>\n      <userinput>echo $?</userinput>\n      <computeroutput>0</computeroutput>\n    </screen>"
msgstr ""

#: xml/ref/pg_isready.xml:218(para)
msgid "Running with connection parameters to a <productname>PostgreSQL</productname>cluster in startup: <screen>\n      <prompt>$</prompt>\n      <userinput>pg_isready -h localhost -p 5433</userinput>\n      <computeroutput>localhost:5433 - rejecting\n      connections</computeroutput>\n      <prompt>$</prompt>\n      <userinput>echo $?</userinput>\n      <computeroutput>1</computeroutput>\n    </screen>"
msgstr ""

#: xml/ref/pg_isready.xml:229(para)
msgid "Running with connection parameters to a non-responsive <productname>PostgreSQL</productname>cluster: <screen>\n      <prompt>$</prompt>\n      <userinput>pg_isready -h someremotehost</userinput>\n      <computeroutput>someremotehost:5432 - no\n      response</computeroutput>\n      <prompt>$</prompt>\n      <userinput>echo $?</userinput>\n      <computeroutput>2</computeroutput>\n    </screen>"
msgstr ""

