#: xml/ref/pg_isready.xml:11(refmiscinfo)
msgid "Application"
msgstr ""

#: xml/ref/pg_isready.xml:16(productname)
msgid "PostgreSQL"
msgstr ""

#: xml/ref/pg_isready.xml:15(refpurpose)
msgid "check the connection status of a <placeholder-1/>server"
msgstr ""

