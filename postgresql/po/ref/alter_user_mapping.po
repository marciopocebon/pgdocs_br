msgid "7"
msgstr ""

#: xml/ref/alter_user_mapping.xml:9(refmiscinfo)
msgid "SQL - Language Statements"
msgstr ""

#: xml/ref/alter_user_mapping.xml:13(refpurpose)
msgid "change the definition of a user mapping"
msgstr ""

#: xml/ref/alter_user_mapping.xml:18(replaceable) xml/ref/alter_user_mapping.xml:42(replaceable)
msgid "user_name"
msgstr ""

#: xml/ref/alter_user_mapping.xml:20(replaceable) xml/ref/alter_user_mapping.xml:55(replaceable)
msgid "server_name"
msgstr ""

#: xml/ref/alter_user_mapping.xml:22(replaceable) xml/ref/alter_user_mapping.xml:64(replaceable)
msgid "option"
msgstr ""

#: xml/ref/alter_user_mapping.xml:23(replaceable) xml/ref/alter_user_mapping.xml:65(replaceable)
