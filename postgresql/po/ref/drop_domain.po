#: xml/ref/drop_domain.xml:21(title)
msgid "Description"
msgstr ""

#: xml/ref/drop_domain.xml:22(para)
msgid "<command>DROP DOMAIN</command>removes a domain. Only the owner of a domain can remove it."
msgstr ""

#: xml/ref/drop_domain.xml:27(title)
msgid "Parameters"
msgstr ""

#: xml/ref/drop_domain.xml:31(literal)
msgid "IF EXISTS"
msgstr ""

#: xml/ref/drop_domain.xml:34(para)
msgid "Do not throw an error if the domain does not exist. A notice is issued in this case."
msgstr ""

#: xml/ref/drop_domain.xml:43(para)
msgid "The name (optionally schema-qualified) of an existing domain."
msgstr ""

#: xml/ref/drop_domain.xml:49(literal)
msgid "CASCADE"
msgstr ""

#: xml/ref/drop_domain.xml:52(para)
msgid "Automatically drop objects that depend on the domain (such as table columns)."
msgstr ""

#: xml/ref/drop_domain.xml:58(literal)
msgid "RESTRICT"
msgstr ""

#: xml/ref/drop_domain.xml:61(para)
msgid "Refuse to drop the domain if any objects depend on it. This is the default."
msgstr ""

