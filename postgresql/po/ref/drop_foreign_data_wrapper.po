#: xml/ref/drop_foreign_data_wrapper.xml:4(primary) xml/ref/drop_foreign_data_wrapper.xml:7(refentrytitle) xml/ref/drop_foreign_data_wrapper.xml:12(refname)
msgid "DROP FOREIGN DATA WRAPPER"
msgstr ""

#: xml/ref/drop_foreign_data_wrapper.xml:8(manvolnum)
msgid "7"
msgstr ""

#: xml/ref/drop_foreign_data_wrapper.xml:9(refmiscinfo)
msgid "SQL - Language Statements"
msgstr ""

#: xml/ref/drop_foreign_data_wrapper.xml:13(refpurpose)
msgid "remove a foreign-data wrapper"
msgstr ""

#: xml/ref/drop_foreign_data_wrapper.xml:17(replaceable) xml/ref/drop_foreign_data_wrapper.xml:41(replaceable)
msgid "name"
msgstr ""

#: xml/ref/drop_foreign_data_wrapper.xml:16(synopsis)
#, no-wrap
msgid "DROP FOREIGN DATA WRAPPER [ IF EXISTS ] \n    <placeholder-1/>[ CASCADE |\n    RESTRICT ]"
msgstr ""

#: xml/ref/drop_foreign_data_wrapper.xml:21(title)
msgid "Description"
msgstr ""

#: xml/ref/drop_foreign_data_wrapper.xml:22(para)
msgid "<command>DROP FOREIGN DATA WRAPPER</command>removes an existing foreign-data wrapper. To execute this command, the current user must be the owner of the foreign-data wrapper."
msgstr ""

#: xml/ref/drop_foreign_data_wrapper.xml:28(title)
msgid "Parameters"
msgstr ""

#: xml/ref/drop_foreign_data_wrapper.xml:32(literal)
msgid "IF EXISTS"
msgstr ""

#: xml/ref/drop_foreign_data_wrapper.xml:35(para)
msgid "Do not throw an error if the foreign-data wrapper does not exist. A notice is issued in this case."
msgstr ""

#: xml/ref/drop_foreign_data_wrapper.xml:44(para)
msgid "The name of an existing foreign-data wrapper."
msgstr ""

#: xml/ref/drop_foreign_data_wrapper.xml:50(literal)
msgid "CASCADE"
msgstr ""

#: xml/ref/drop_foreign_data_wrapper.xml:53(para)
msgid "Automatically drop objects that depend on the foreign-data wrapper (such as servers)."
msgstr ""

#: xml/ref/drop_foreign_data_wrapper.xml:59(literal)
msgid "RESTRICT"
msgstr ""

#: xml/ref/drop_foreign_data_wrapper.xml:62(para)
msgid "Refuse to drop the foreign-data wrappers if any objects depend on it. This is the default."
msgstr ""

#: xml/ref/drop_foreign_data_wrapper.xml:69(title)
msgid "Examples"
msgstr ""

#: xml/ref/drop_foreign_data_wrapper.xml:72(programlisting)
#, no-wrap
msgid "DROP FOREIGN DATA WRAPPER\n    dbi;"
msgstr ""

#: xml/ref/drop_foreign_data_wrapper.xml:70(para)
msgid "Drop the foreign-data wrapper <literal>dbi</literal>: <placeholder-1/>"
msgstr ""

#: xml/ref/drop_foreign_data_wrapper.xml:76(title)
msgid "Compatibility"
msgstr ""

#: xml/ref/drop_foreign_data_wrapper.xml:77(para)
msgid "<command>DROP FOREIGN DATA WRAPPER</command>conforms to ISO/IEC 9075-9 (SQL/MED). The <literal>IF EXISTS</literal>clause is a <productname>PostgreSQL</productname>extension."
msgstr ""

