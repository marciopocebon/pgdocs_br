#: xml/ref/alter_conversion.xml:16(synopsis)
#, no-wrap
msgid "ALTER CONVERSION \n    <placeholder-1/>RENAME TO \n    <placeholder-2/>ALTER CONVERSION \n    <placeholder-3/>OWNER TO { \n    <placeholder-4/>| CURRENT_USER |\n    SESSION_USER } ALTER CONVERSION \n    <placeholder-5/>SET SCHEMA \n    <placeholder-6/>"
msgstr ""

#: xml/ref/alter_conversion.xml:26(title)
msgid "Description"
msgstr ""

#: xml/ref/alter_conversion.xml:27(para)
msgid "<command>ALTER CONVERSION</command>changes the definition of a conversion."
msgstr ""

#: xml/ref/alter_conversion.xml:30(para)
msgid "You must own the conversion to use <command>ALTER CONVERSION</command>. To alter the owner, you must also be a direct or indirect member of the new owning role, and that role must have <literal>CREATE</literal>privilege on the conversion's schema. (These restrictions enforce that altering the owner doesn't do anything you couldn't do by dropping and recreating the conversion. However, a superuser can alter ownership of any conversion anyway.)"
msgstr ""

#: xml/ref/alter_conversion.xml:41(title)
msgid "Parameters"
msgstr ""

#: xml/ref/alter_conversion.xml:48(para)
msgid "The name (optionally schema-qualified) of an existing conversion."
msgstr ""

#: xml/ref/alter_conversion.xml:57(para)
msgid "The new name of the conversion."
msgstr ""

#: xml/ref/alter_conversion.xml:65(para)
msgid "The new owner of the conversion."
msgstr ""

#: xml/ref/alter_conversion.xml:73(para)
msgid "The new schema for the conversion."
msgstr ""

#: xml/ref/alter_conversion.xml:79(title)
msgid "Examples"
msgstr ""

#: xml/ref/alter_conversion.xml:83(programlisting)
#, no-wrap
msgid "ALTER CONVERSION iso_8859_1_to_utf8 RENAME TO\n    latin1_to_unicode;"
msgstr ""

#: xml/ref/alter_conversion.xml:80(para)
msgid "To rename the conversion <literal>iso_8859_1_to_utf8</literal>to <literal>latin1_to_unicode</literal>: <placeholder-1/>"
msgstr ""

#: xml/ref/alter_conversion.xml:88(programlisting)
#, no-wrap
msgid "ALTER CONVERSION iso_8859_1_to_utf8 OWNER TO\n    joe;"
msgstr ""

#: xml/ref/alter_conversion.xml:85(para)
msgid "To change the owner of the conversion <literal>iso_8859_1_to_utf8</literal>to <literal>joe</literal>: <placeholder-1/>"
msgstr ""

