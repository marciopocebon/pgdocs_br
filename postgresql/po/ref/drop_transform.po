msgstr ""

#: xml/ref/drop_transform.xml:37(para)
msgid "Do not throw an error if the transform does not exist. A notice is issued in this case."
msgstr ""

#: xml/ref/drop_transform.xml:46(para)
msgid "The name of the data type of the transform."
msgstr ""

#: xml/ref/drop_transform.xml:54(para)
msgid "The name of the language of the transform."
msgstr ""

#: xml/ref/drop_transform.xml:59(literal)
msgid "CASCADE"
msgstr ""

#: xml/ref/drop_transform.xml:62(para)
