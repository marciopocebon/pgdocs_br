#: xml/ref/createdb.xml:16(productname)
msgid "PostgreSQL"
msgstr ""

#: xml/ref/createdb.xml:15(refpurpose)
msgid "create a new <placeholder-1/>database"
msgstr ""

#: xml/ref/createdb.xml:22(replaceable)
msgid "connection-option"
msgstr ""

